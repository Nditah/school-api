{
   "name": "school-user-api",
   "version": "0.0.1",
   "description": "User microsservice for Education Management Information System.",
   "private": true,
   "main": "dist/index.js",
   "scripts": {
      "tsc": "tsc",
      "start": "ts-node src/server.ts",
      "lint": "eslint --fix --ext .ts,.js ./src",
      "dev": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/server.ts --experimental-modules",
      "prod": "tsc && node ./build/index.js --experimental-modules",
      "test": "mocha -r ts-node/register src/**/*.spec.ts",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/helmet": "0.0.45",
      "@types/jsonwebtoken": "^8.3.5",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "chai": "^4.2.0",
      "class-validator": "^0.11.0",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "helmet": "^3.21.2",
      "jsonwebtoken": "^8.5.1",
      "mocha": "^7.0.0",
      "morgan": "^1.9.1",
      "mysql2": "^2.1.0",
      "reflect-metadata": "^0.1.10",
      "ts-node-dev": "^1.0.0-pre.44",
      "typeorm": "0.2.22"
   },
   "devDependencies": {
      "@types/chai": "^4.2.7",
      "@types/express": "^4.17.2",
      "@types/mocha": "^5.2.7",
      "@types/node": "^8.10.59",
      "@typescript-eslint/eslint-plugin": "^2.15.0",
      "@typescript-eslint/parser": "^2.15.0",
      "eslint": "^6.8.0",
      "eslint-config-airbnb-base": "^14.0.0",
      "eslint-config-prettier": "^6.9.0",
      "eslint-plugin-import": "^2.19.1",
      "eslint-plugin-prettier": "^3.1.2",
      "prettier": "^1.19.1",
      "ts-node": "^3.3.0",
      "typescript": "^3.7.2"
   }
}
